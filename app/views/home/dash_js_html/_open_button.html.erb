  <style>
  	.wittyEDContainer {
			position: fixed;
	    top: 0;
	    left: 0;
	    z-index: 999999;
  	}

  	.wittyEDOpenButton {
	    background: rgba(49, 55, 61, 0.4);
	    width: 75px;
	    height: 60px;
	    padding: 20px;
	    cursor: pointer;
			z-index: 9;
	    position: relative;
	    box-sizing: border-box;
  	}

  	.slide-out {
  		background: rgba(49, 55, 61, 0.4);
	    position: absolute;
	    left: 75px;
	    width: 20px;
	    height: 60px;
	    top: 0;
  	}

    .wittyEDOpenButton .svg_dark {
    	fill: #fff;
    }

    #wittyEDselectWindow {
			border: none;
	    position: absolute;
	    top: 0;
	    left: 0;
	    width: 75px;
	    height: 60px;
	    opacity: 0;
	    transition: .5s width, .5s height;
    }

    .wittyEDOpenButton.open ~ #wittyEDselectWindow {
    	opacity: 1;
    	width: 300px;
	    height: 548px;
      z-index: 7;
    }
  </style>

  <iframe id="wittyEDselectWindow" src="<%= Rails.env.production? ? 'https://easydash.herokuapp.com' : 'https://localhost:3000' %>/preview-window"></iframe>
