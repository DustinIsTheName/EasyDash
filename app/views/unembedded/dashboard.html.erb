        <article>
          <div class="card collection_conditions">
            <h5>Conditions</h5>
            <div class="row extra_margin_3x">
              <label class="extra_margin"><input type="radio" name="option2">Manually select products <span class="subdued">(you will be able to select products on the next page)</span></label>
              <label><input type="radio" name="option2" checked="checked">Automatically select products based on conditions</label>
            </div>
            <div class="row">
              <label>Products must match:</label>
              <label><input type="radio" name="option3" checked="checked">all conditions</label>
              <label><input type="radio" name="option3">any condition</label>
            </div>
            <div class="divider"></div>
            <div class="row">
              <select class="extra_margin" id="collection_rules_column">
                <option selected="selected" value="title">Product title</option>
				<option value="type">Product type</option>
				<option value="vendor">Product vendor</option>
				<option value="variant_price">Product price</option>
				<option value="tag">Product tag</option>
				<option value="variant_compare_at_price">Compare at price</option>
				<option value="variant_weight">Weight</option>
				<option value="variant_inventory">Inventory stock</option>
				<option value="variant_title">Variant's title</option>
              </select>
              <select class="extra_margin" id="rule_relation">
                <option value="equals" selected="selected" data-bind-disabled="isRelationDisabled(this.value)">is equal to</option>
                <option value="not_equals" data-bind-disabled="isRelationDisabled(this.value)">is not equal to</option>
                <option value="greater_than" data-bind-disabled="isRelationDisabled(this.value)" disabled="">is greater than</option>
                <option value="less_than" data-bind-disabled="isRelationDisabled(this.value)" disabled="">is less than</option>
                <option value="starts_with" data-bind-disabled="isRelationDisabled(this.value)">starts with</option>
                <option value="ends_with" data-bind-disabled="isRelationDisabled(this.value)">ends with</option>
                <option value="contains" data-bind-disabled="isRelationDisabled(this.value)">contains</option>
                <option value="not_contains" data-bind-disabled="isRelationDisabled(this.value)">does not contain</option>
              </select>
              <input type="text" />
            </div>
            <div class="divider" style="margin-left: 0; margin-right: 0;"></div>
            <button class="secondary">Add another condition</button>
          </div>
        </article>
